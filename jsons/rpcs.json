{
  "mvm": {
    "estimateGasExecute": {
      "aliasSection": "mvm",
      "description": "estimate gas for script execution",
      "params": [
        {
          "name": "account",
          "type": "AccountId"
        },
        {
          "name": "tx_bc",
          "type": "Bytes"
        },
        {
          "name": "gas_limit",
          "type": "u64"
        }
      ],
      "type": "Estimation"
    },
    "estimateGasPublish": {
      "aliasSection": "mvm",
      "description": "estimate gas for module publishing",
      "params": [
        {
          "name": "account",
          "type": "AccountId"
        },
        {
          "name": "module_bc",
          "type": "Bytes"
        },
        {
          "name": "gas_limit",
          "type": "u64"
        }
      ],
      "type": "Estimation"
    },
    "gasToWeight": {
      "aliasSection": "mvm",
      "description": "convert gas to weight",
      "params": [
        {
          "name": "gas",
          "type": "u64"
        }
      ],
      "type": "Weight"
    },
    "weightToGas": {
      "aliasSection": "mvm",
      "description": "convert gas to weight",
      "params": [
        {
          "name": "weight",
          "type": "Weight"
        }
      ],
      "type": "u64"
    }
  }
}
